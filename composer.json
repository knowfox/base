{
    "name": "knowfox/base",
    "type": "project",
    "description": "Knowfox. A personal knowledge management system.",
    "keywords": [
        "knowfox",
        "personal knowledge management"
    ],
    "license": "GPLv3",
    "repositories": [
        {
            "type": "path",
            "url": "packages/knowfox/knowfox",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/knowfox/entangle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/knowfox/pocket",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/knowfox/drupal7",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.3",
        "duellsy/pockpack": "^2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "knowfox/knowfox": "dev-minimal-change",
        "laravel/framework": "^8.0",
        "laravel/jetstream": "^1.4",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^2.0",
        "symfony/event-dispatcher": "^5.2"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
